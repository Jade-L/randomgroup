<p id="notice"><%= notice %></p>

<h1>Listing Personnes</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Email</th>
      <th>Tel</th>
      <th>Group</th>
      <th>Sensei</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personnes.each do |personne| %>
      <tr>
        <td><%= personne.nom %></td>
        <td><%= personne.email %></td>
        <td>0<%= personne.tel %></td>
        <td><%= personne.group_id %></td>
        <td><%= personne.sensei %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
